[production]
# Замените на ваши реальные серверы
django-server-1 ansible_host=your-server-ip ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/your-key.pem

[staging]
# Для тестирования
staging-server ansible_host=staging-server-ip ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/staging-key.pem

[webservers:children]
production
staging

[dbservers]
# Если база данных на отдельном сервере
db-server ansible_host=db-server-ip ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/db-key.pem
